                <TextBlock Name="dataRightLocation" Text="{Binding DataRightLocation}" Width="250" FontSize="18"/>
            </StackPanel>
            <StackPanel Grid.Row="12" Orientation="Horizontal" HorizontalAlignment="Left" >
