                                            <TextBlock Grid.Column="3" Text="Layer 3" HorizontalAlignment="Center" Foreground="White"></TextBlock>
                                            <TextBlock Grid.Column="4" Text="Layer 4" HorizontalAlignment="Center" Foreground="White"></TextBlock>
                                            <TextBlock Grid.Column="5" Text="Layer 5" HorizontalAlignment="Center" Foreground="White"></TextBlock>
                                            <TextBlock Grid.Column="6" Text="Layer 6" HorizontalAlignment="Center" Foreground="White"></TextBlock>
                                            <TextBlock Grid.Column="7" Text="Layer 7" HorizontalAlignment="Center" Foreground="White"></TextBlock>
