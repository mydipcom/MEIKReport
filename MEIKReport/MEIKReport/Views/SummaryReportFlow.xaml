                                            <TextBlock Grid.Column="2" Text="11mm" HorizontalAlignment="Center"></TextBlock>
                                            <TextBlock Grid.Column="3" Text="18mm" HorizontalAlignment="Center"></TextBlock>
                                            <TextBlock Grid.Column="4" Text="25mm" HorizontalAlignment="Center"></TextBlock>
                                            <TextBlock Grid.Column="5" Text="32mm" HorizontalAlignment="Center"></TextBlock>
                                            <TextBlock Grid.Column="6" Text="39mm" HorizontalAlignment="Center"></TextBlock>
