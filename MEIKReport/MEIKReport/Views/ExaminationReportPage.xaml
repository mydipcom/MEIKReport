                            <TextBlock Text="Gender: " FontWeight="Bold" Margin="5,0,0,0"/>
                            <ComboBox Name="dataGender" Text="{Binding DataGender}" Width="110" BorderBrush="Gray" Background="{x:Null}" FontSize="10">
                                <ComboBoxItem IsSelected="True"></ComboBoxItem>
                                <ComboBoxItem>Female</ComboBoxItem>
                                <ComboBoxItem>Male</ComboBoxItem>
                            </ComboBox>                            
