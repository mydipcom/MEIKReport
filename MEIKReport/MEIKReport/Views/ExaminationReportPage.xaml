                            <!--<TextBlock Name="dataName" Text="{Binding DataName}" Width="230" TextDecorations="Underline" FontSize="10"/>-->
                            <TextBlock Text="Screening Date: " FontWeight="Bold" Margin="20,0,0,0"/>
