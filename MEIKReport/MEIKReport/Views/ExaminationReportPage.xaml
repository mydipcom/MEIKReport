                        <StackPanel Grid.Row="5" Orientation="Horizontal" HorizontalAlignment="Left" >
                            <TextBlock Text="Address: " FontWeight="Bold"/>
                            <TextBox Name="dataAddress" Text="{Binding DataAddress}" VerticalAlignment="Center" Width="380" Height="18" BorderBrush="Gray" Background="{x:Null}" Margin="0,0,5,0" FontSize="10"></TextBox>
                            <!--<TextBlock Name="dataAddress" Text="{Binding DataAddress}" Width="380" Height="18" Margin="0,0,5,0" FontSize="10"></TextBlock>-->
                            <!--<TextBlock Text="" Width="380" TextDecorations="Underline"/>-->
                            <TextBlock Text="Health insurance card: " FontWeight="Bold"/>
                            <TextBox Name="dataHealthCard" Text="{Binding DataHealthCard}" Width="180" Height="18" BorderBrush="Gray" Background="{x:Null}" FontSize="10"></TextBox>
                            <!--<TextBlock Text="" Width="150"/>-->
                        </StackPanel>
                        <StackPanel Grid.Row="6" Orientation="Horizontal" HorizontalAlignment="Left" >
                            <TextBlock Text="MRN# (Code):  " FontWeight="Bold"/>
