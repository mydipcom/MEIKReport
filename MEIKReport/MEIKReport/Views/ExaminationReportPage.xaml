                                <ComboBoxItem>Lbs</ComboBoxItem>
                                <ComboBoxItem IsSelected="True">Kgs</ComboBoxItem>                                
                            </ComboBox>
                            <!--<TextBlock Text="" Width="102"/>-->
                            <TextBlock Text="Menstrual cycle: " FontWeight="Bold"/>
