<Window x:Class="MEIKReport.UserList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cvt="clr-namespace:MEIKReport.Common"
        Title="MEIK Application" Height="800" Width="1084"
        Icon="/MEIKReport;component/Images/App_Icon.png"
        ShowInTaskbar="True" ResizeMode="CanResize"  Closed="Window_Closed" WindowStartupLocation="CenterScreen" WindowState="Maximized">    
    <!--<Window.TaskbarItemInfo>
        <TaskbarItemInfo Description="MEIK Homepage" Overlay="/MEIKReport;component/Images/logo.jpg" ProgressState="Normal"  ProgressValue="10" />
    </Window.TaskbarItemInfo>-->
    <Window.Resources>        
        <CollectionViewSource x:Key="CustomerSource" />
        <CollectionViewSource x:Key="ProcessBarSource" />
        <cvt:ReverseTypeConverter x:Key="reverseConverter"/>
    </Window.Resources>
    <Grid>        
        <Grid.Background>
            <ImageBrush ImageSource="../Images/bg.jpg"  />
        </Grid.Background>
        <!--<Grid.Resources>            
            <XmlDataProvider x:Key="LanguageDataSource" XPath="Locals">
                <x:XData>
                    <Locals xmlns="">
                        <Local Name="English" Flag="en-US">                            
                        </Local>
                        <Local Name="繁體中文" Flag="zh-HK">                            
                        </Local>
                        --><!--<Local Name="简体中文" Flag="zh-CN">                         
                        </Local>--><!--                        
                    </Locals>
                </x:XData>
            </XmlDataProvider>            
        </Grid.Resources>-->
        <Grid.RowDefinitions>
            <RowDefinition Height="130"/>
            <RowDefinition Height="2"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>            
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>                
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="300"/>                
            </Grid.ColumnDefinitions>
            <WrapPanel VerticalAlignment="Center" Margin="30,0,0,0">                
                <Button Name="btnScreening"  Content="{DynamicResource  MainWinMenuScreening}" Height="85" Width="110" FontWeight="Bold" VerticalContentAlignment="Top" VerticalAlignment="Top" HorizontalContentAlignment="Center"  FontFamily="Arial,Microsoft YaHei" FontSize="15" Click="btnScreening_Click"  Foreground="White" Cursor="Hand">                                     
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/screening.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>                                                                
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/screening_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>                                
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="btnDiagnostics" Content="{DynamicResource  MainWinMenuDiagnostics}" Width="110" Height="85" Foreground="White" FontWeight="Bold" FontSize="15" FontFamily="Arial,Microsoft YaHei" VerticalAlignment="Top" VerticalContentAlignment="Top" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Click="btnDiagnostics_Click" Cursor="Hand">                    
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/diagnostics.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/diagnostics_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                                <Trigger Property="Button.IsPressed" Value="True">
                                    <Setter Property="Source" Value="/Images/diagnostics_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>                        
                    </Button.Template>
                    
                </Button>
                <Button Name="btnRecords" Content="{DynamicResource  MainWinMenuRecords}" Height="85" Width="110" FontFamily="Arial,Microsoft YaHei" FontSize="15" FontWeight="Bold" Click="btnRecords_Click" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/records.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/records_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="btnSetup" Content="{DynamicResource  MainWinMenuPerferences}" Height="85" Width="120"  FontFamily="Arial,Microsoft YaHei" FontSize="15" FontWeight="Bold" Click="btnSetup_Click" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/setting.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/setting_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="btnReceive" Content="{DynamicResource  MainWinMenuReceive}" Height="85" Width="120"  FontFamily="Arial,Microsoft YaHei" FontSize="15" FontWeight="Bold"  Foreground="White" Cursor="Hand" Click="btnReceive_Click" >
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/receive.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/receive_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="btnReceivePdf" Content="{DynamicResource  MainWinMenuReceiveReport}" Height="85" Width="120"  FontFamily="Arial,Microsoft YaHei" FontSize="15" FontWeight="Bold"  Foreground="White" Cursor="Hand" Click="btnReceivePdf_Click" >
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/receive.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Center"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </StackPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/receive_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
            </WrapPanel>
            <Grid Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Width="300">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="30"/>
                    <RowDefinition Height="*"/>                    
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="250"/>                    
                    <ColumnDefinition Width="30"/>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center" Width="280" Height="60">
                    <Label Content="{DynamicResource  MainWinDeviceNo}" Width="240" Height="60" VerticalContentAlignment="Center" HorizontalAlignment="Right" HorizontalContentAlignment="Right" FontSize="18" Foreground="White" FontWeight="Bold" FontFamily="Arial,Microsoft YaHei"  ></Label>
                    <Label Name="labDeviceNo" Content="001" Width="40" Height="60" VerticalContentAlignment="Center" HorizontalContentAlignment="left" FontSize="18" Foreground="White" FontWeight="Bold" FontFamily="Arial,Microsoft YaHei"></Label>
                </StackPanel>

                <StackPanel Grid.Row="2" Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right" Width="145" Height="30">
                    <Button Name="languageUS" HorizontalAlignment="Center" VerticalAlignment="Center" Content="English" Width="70"  FontSize="14" Foreground="White" FontWeight="Bold" Click="Language_Click" Cursor="Hand">
                        <Button.Template>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                    <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                                </StackPanel>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="Button.IsMouseOver" Value="True">
                                        <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                    </Trigger>
                                    <Trigger Property="Button.IsPressed" Value="True">
                                        <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    <Label Width="1" Height="16" Background="White"></Label>
                    <Button Name="languageHK" HorizontalAlignment="Center" VerticalAlignment="Center" Content="繁體中文" Width="70"  FontSize="14" Foreground="White" FontWeight="Bold" Click="Language_Click" Cursor="Hand">
                        <Button.Template>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <StackPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="Center" VerticalAlignment="Top" OpacityMask="#FFE81717">
                                    <Label Name="btnTxt" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                                </StackPanel>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="Button.IsMouseOver" Value="True">
                                        <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                    </Trigger>
                                    <Trigger Property="Button.IsPressed" Value="True">
                                        <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Button.Template>
                    </Button>
                    <!--<ComboBox Name="listLang" Width="200" Height="20" BorderBrush="White" Background="White"  ItemsSource="{Binding Source={StaticResource LanguageDataSource}, XPath=Local}" DisplayMemberPath="@Name" FontSize="12" SelectionChanged="listLang_SelectionChanged">                                    
                    </ComboBox>-->
                </StackPanel>
            </Grid>
        </Grid>
        <Grid Grid.Row="1" Background="White">            
        </Grid>
        <Grid Grid.Row="2" Margin="10,5,10,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="160"/>
                <ColumnDefinition Width="415"/>                
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <StackPanel Grid.Column="0" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,5,0">
                <Label Content="{DynamicResource  UserListWinBrowerLab}" Width="150" HorizontalContentAlignment="Right" FontSize="13" FontWeight="Bold" Foreground="White"></Label>                
            </StackPanel>
            <WrapPanel Grid.Column="1"  HorizontalAlignment="Left" VerticalAlignment="Center">
                <TextBox Name="txtFolderPath" Width="295" IsEnabled="False" Height="26" VerticalAlignment="Center" VerticalContentAlignment="Center"></TextBox>
                <Button Name="btnSelectFolder" Width="110" Height="42" Content="{DynamicResource  UserListWinBrowerBtn}" FontSize="13" FontWeight="Bold" FontFamily="Arial,Microsoft YaHei"  Click="btnSelectFolder_Click" Background="{x:Null}" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <WrapPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="left" VerticalAlignment="Center" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/browse.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Left"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Margin="-6,0,0,0" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </WrapPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/browse_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
            </WrapPanel>            
            <WrapPanel Name="reportButtonPanel" Grid.Column="2" HorizontalAlignment="left" VerticalAlignment="Center">
                <Button Name="btnNewArchive" Width="90" Height="42" Content="{DynamicResource  UserListWinNewBtn}" FontSize="13" FontWeight="Bold" FontFamily="Arial,Microsoft YaHei" Click="btnNewArchive_Click" Background="{x:Null}" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <WrapPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="left" VerticalAlignment="Center" OpacityMask="#FFE81717">
                                <Image Name="btnBG" Source="/Images/new.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Left"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Margin="-6,0,0,0" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </WrapPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/new_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>

                <Button Name="btnExaminationReport" Click="ExaminationReport_Click" Width="126" Height="42" FontSize="13" FontWeight="Bold" HorizontalContentAlignment="Left" HorizontalAlignment="Left" Content="{DynamicResource UserListWinEditReportBtn}" Background="{x:Null}" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <WrapPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="left" VerticalAlignment="Center">
                                <Image Name="btnBG" Source="/Images/edit.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Left"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Margin="-6,0,0,0" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </WrapPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/edit_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="btnSummaryReport" Click="SummaryReport_Click" Width="126" Height="42" Style="{StaticResource NewButtonStyle}" Margin="0,0,8,0" FontSize="14" Content="{DynamicResource UserListWinSummaryReportBtn}" Visibility="Collapsed"></Button>
                <Button Name="sendDataButton" Width="120" Height="42" FontSize="13" FontWeight="Bold" HorizontalContentAlignment="Left" HorizontalAlignment="Left" Click="SendDataButton_Click" Content="{DynamicResource UserListWinEmailDataBtn}" Background="{x:Null}" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <WrapPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="left" VerticalAlignment="Center">
                                <Image Name="btnBG" Source="/Images/send.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Left"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Margin="-6,0,0,0" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </WrapPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/send_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
                <Button Name="sendReportButton" Width="130" Height="42" FontSize="13" FontWeight="Bold" HorizontalContentAlignment="Left" HorizontalAlignment="Left" Click="SendReportButton_Click" Content="{DynamicResource UserListWinEmailReportBtn}" Background="{x:Null}" Foreground="White" Cursor="Hand">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <WrapPanel Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" HorizontalAlignment="left" VerticalAlignment="Center">
                                <Image Name="btnBG" Source="/Images/send.png" Stretch="None" Width="auto" Height="auto" HorizontalAlignment="Left"/>
                                <!--<ContentPresenter VerticalAlignment="Center" HorizontalAlignment="Center" Content="{TemplateBinding Content}"></ContentPresenter>-->
                                <Label Name="btnTxt" Margin="-6,0,0,0" Content="{TemplateBinding Content}" Foreground="{TemplateBinding Foreground}" FontWeight="{TemplateBinding FontWeight}" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalAlignment}" HorizontalAlignment="{TemplateBinding HorizontalAlignment}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"></Label>
                            </WrapPanel>
                            <ControlTemplate.Triggers>
                                <Trigger Property="Button.IsMouseOver" Value="True">
                                    <Setter Property="Source" Value="/Images/send_hover.png" TargetName="btnBG"/>
                                    <Setter Property="Foreground" Value="#40c4e4" TargetName="btnTxt"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
            </WrapPanel>
        </Grid>
        <Grid Name="dataGrid" Grid.Row="3" DataContext="{Binding Source={StaticResource CustomerSource}}" Margin="20,0,20,0" Background="white">
            <Grid.RowDefinitions>
                <RowDefinition Height="0"/>
                <RowDefinition Height="30"/>
                <RowDefinition Height="*"/>                
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="320"/>                
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            
            <!--<Border  Grid.ColumnSpan="2" BorderBrush="Black" BorderThickness="0,2,0,0"></Border>-->
            <Grid Grid.Row="1" Grid.Column="0" Background="#FF328787" Margin="0,0,1,0">                
                <StackPanel  VerticalAlignment="Center">
                    <TextBlock Text="{DynamicResource  ReportContext_178}" TextAlignment="Center" FontSize="12" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
                <Image Name="imgChoice" Width="20" Height="20" Margin="15,0,0,0" HorizontalAlignment="Left" Source="/Images/multiple_choice.png" MouseUp="imgChoice_MouseUp" Cursor="Hand"/>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="1" Background="#FF328787" >
                <StackPanel  VerticalAlignment="Center">
                    <TextBlock Text="{DynamicResource  UserListWinPatientCardTitle}" TextAlignment="Center" FontSize="12" FontWeight="Bold" Foreground="White"/>
                </StackPanel>
            </Grid>
            
            <ListBox Grid.Row="2" Grid.Column="0" Name="CodeListBox" IsSynchronizedWithCurrentItem="True" SelectionMode="Extended" ItemsSource="{Binding}" Margin="5,2,2,5" SelectionChanged="CodeListBox_SelectionChanged">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Name="MRNListItem" Orientation="Horizontal" Height="25">
                            <Image Focusable="False" Width="22" Height="22" Source="{Binding Path=Icon}" />
                            <TextBlock Text="{Binding Path=Code}" Width="90" VerticalAlignment="Center" Margin="5,0,0,0"/>
                            <TextBlock Text="{Binding Path=SurName}" Width="170" VerticalAlignment="Center"/>
                            <Image Focusable="False"  Width="18" Height="18" Source="/Images/sended.png" VerticalAlignment="Center" HorizontalAlignment="Right" Visibility="{Binding Path=Uploaded}"/>
                        </StackPanel>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ListBox>
            <TabControl Name="tabGroup" Grid.Row="2" Grid.Column="1" SelectedIndex="0" Margin="2,2,5,5" Background="{x:Null}">
                <TabItem Name="tabPersonal" Header="{DynamicResource UserDataPersonalTab}" Height="Auto">
                    <Grid Grid.Row="1" Margin="5,10,5,0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="15"/>
                            <RowDefinition Height="26"/>
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="180"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="40"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="130"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Width="Auto" HorizontalContentAlignment="Right" Content="{DynamicResource UserDataPersonal1}"></Label>
                        <Label Grid.Row="1" Grid.Column="0" Width="Auto" HorizontalContentAlignment="Right" Content="{DynamicResource UserDataPersonal2}"></Label>
                        <Label Grid.Row="2" Grid.Column="0" Width="Auto" HorizontalContentAlignment="Right" Content="{DynamicResource UserDataPersonal3}"></Label>
                        <WrapPanel Grid.Row="3" Grid.Column="1">
                            <Label Width="60" HorizontalContentAlignment="Center" Content="{DynamicResource UserDataPersonal7}"></Label>
